{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,mjs,json,scss,mdx,yml,yaml}'",
    "format": "prettier --check '**/*.{ts,tsx,js,mjs,json,scss,mdx,yml,yaml}'",
    "lint": "eslint --ext .ts,.tsx,.js,mjs \"./\" --quiet --cache --color",
    "lint:fix": "yarn lint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "yarn format:fix",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "12.1.6",
    "next-themes": "^0.2.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.3.1",
    "use-local-storage": "^2.3.6"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "8.10.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-next": "12.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "husky": "4.3.8",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "10",
    "typescript": "4.6.4"
  }
}
