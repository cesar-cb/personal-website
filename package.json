{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,mjs,json,scss,mdx,yml,yaml}'",
    "format": "prettier --check '**/*.{ts,tsx,js,mjs,json,scss,mdx,yml,yaml}'",
    "lint": "eslint --ext .ts,.tsx,.js,mjs \"./\" --quiet --cache --color",
    "lint:fix": "yarn lint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "yarn format:fix",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@notionhq/client": "^2.2.3",
    "classnames": "^2.3.1",
    "next": "13.2.1",
    "next-themes": "^0.2.0",
    "notion-to-md": "^2.5.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.5",
    "rehype-raw": "^6.1.1",
    "use-local-storage": "^2.3.6"
  },
  "devDependencies": {
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "husky": "8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.5.1",
    "sass": "^1.58.3",
    "sass-loader": "13.2.0",
    "typescript": "4.9.5"
  }
}
