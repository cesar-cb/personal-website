{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,mjs,json,scss,mdx,yml,yaml}'",
    "format": "prettier --check '**/*.{ts,tsx,js,mjs,json,scss,mdx,yml,yaml}'",
    "lint": "eslint --ext .ts,.tsx,.js,mjs \"./\" --quiet --cache --color",
    "lint:fix": "yarn lint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "yarn format:fix",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@notionhq/client": "^2.2.13",
    "@types/lodash": "^4.14.199",
    "@types/react-syntax-highlighter": "^15.5.7",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "next": "13.5.3",
    "next-themes": "^0.2.0",
    "nextjs-progressbar": "^0.0.16",
    "notion-to-md": "^2.5.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-markdown": "^8.0.7",
    "react-node-to-string": "^0.1.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1",
    "remark-unwrap-images": "^3.0.1",
    "sharp": "^0.32.6",
    "use-local-storage": "^2.3.6"
  },
  "devDependencies": {
    "@types/node": "18.18.0",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "sass-loader": "13.3.2",
    "typescript": "5.2.2"
  }
}
